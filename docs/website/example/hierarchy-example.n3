@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

<http://example.com/Shared+Services>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Corporate> .

<http://example.com/HR>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Shared+Services> .

<http://example.com/Company+USA>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Americas> .

<http://example.com/Sales+Germany>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Company+Germany> .

<http://example.com/IT>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Shared+Services> .

<http://example.com/Marketing+Germany>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Company+Germany> .

<http://example.com/Sales+USA>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Company+USA> .

<http://example.com/EMEA>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Corporate> .

<http://example.com/Company+Germany>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/EMEA> .

<http://example.com/Americas>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Corporate> .

<http://example.com/Corporate>
      rdf:type rdfs:Class .

<http://example.com/Marketing+USA>
      rdf:type rdfs:Class ;
      rdfs:subClassOf <http://example.com/Company+USA> .
